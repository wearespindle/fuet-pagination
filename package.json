{
  "name": "fuet-pagination",
  "version": "1.0.5",
  "description": "A simple pagination component for Vue 2.",
  "main": "src/js/index.js",
  "peerDependencies": {
    "bulma": ">=0.5",
    "vue": ">=2"
  },
  "watch": {
    "vue": {
      "patterns": [
        "src",
        "vue"
      ],
      "extensions": "vue",
      "quiet": false
    }
  },
  "scripts": {
    "vue": "fuet -e -i 'src/vue/*.vue' -p src vue -o src/js/templates.mjs",
    "prepublish": "fuet -e -i 'src/vue/*.vue' -p src vue -o src/js/templates.mjs",
    "watch": "npm-watch"
  },
  "files": [
    "src/js/*.js",
    "LICENSE",
    "README.md"
  ],
  "repository": "https://github.com/wearespindle/fuet-pagination",
  "keywords": [
    "vue",
    "fuet",
    "pagination"
  ],
  "author": "Devhouse Spindle",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^4.4.1",
    "eslint-plugin-sort-class-members": "^1.2.0",
    "npm-watch": "^0.1.9"
  }
}
